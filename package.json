{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "frontend-build": "rollup -c",
    "frontend-dev": "rollup -c -w",
    "frontend-start": "sirv public --no-clear",
    "frontend-check": "svelte-check --tsconfig ./tsconfig.json",
    "backend-build": "cd backend && go build",
    "backend-dev": "cd backend && nodemon --signal SIGINT --exec 'go run . || exit 1' -e go",
    "dev": "concurrently npm:frontend-dev npm:backend-dev",
    "format": "pnpm prettier frontend/ --write --plugin-search-dir=."
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^3.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/colornames": "^1.1.2",
    "concurrently": "^6.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rollup": "^2.67.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@geoffcox/svelte-splitter": "^1.0.1",
    "@tiptap/core": "^2.0.0-beta.174",
    "@tiptap/extension-bubble-menu": "^2.0.0-beta.55",
    "@tiptap/extension-image": "^2.0.0-beta.27",
    "@tiptap/extension-link": "^2.0.0-beta.36",
    "@tiptap/extension-placeholder": "^2.0.0-beta.48",
    "@tiptap/starter-kit": "^2.0.0-beta.183",
    "base64-arraybuffer": "^1.0.2",
    "colornames": "^1.1.1",
    "deep-equal": "^2.0.5",
    "just-diff": "^5.0.1",
    "rfdc": "^1.3.0",
    "sirv-cli": "^1.0.14",
    "svelte-grid": "^5.1.1",
    "svelte-json-tree": "^0.1.0",
    "svelte-simple-modal": "^1.3.1",
    "sveltejs-tippy": "^3.0.0",
    "tinykeys": "^1.4.0",
    "uuid": "^8.3.2"
  }
}
