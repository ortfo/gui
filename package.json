{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "frontend-build": "vite build",
    "frontend-dev": "vite --port 3000",
    "frontend-start": "vite preview",
    "frontend-check": "svelte-check --tsconfig ./tsconfig.json",
    "backend-build": "cd backend && go build",
    "backend-dev": "cd backend && nodemon --signal SIGINT --exec 'DEBUG=1 DEV=yes go run . || exit 1' -e go",
    "dev": "concurrently -i npm:frontend-dev npm:backend-dev",
    "format": "pnpm prettier frontend/ --write --plugin-search-dir=. && cd backend && gofmt -s -w *.go"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-yaml": "^4.1.2",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/colornames": "^1.1.4",
    "@types/lodash": "^4.17.0",
    "@types/mime-db": "^1.43.5",
    "c8": "^7.14.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.57.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.75.0",
    "svelte": "^4.2.15",
    "svelte-check": "^2.10.3",
    "svelte-notifications": "^0.9.98",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^4.1.0",
    "@geoffcox/svelte-splitter": "^1.0.1",
    "@ortfo/db": "^1.5.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@tiptap/core": "^2.3.0",
    "@tiptap/extension-bubble-menu": "^2.3.0",
    "@tiptap/extension-image": "^2.3.0",
    "@tiptap/extension-link": "^2.3.0",
    "@tiptap/extension-placeholder": "^2.3.0",
    "@tiptap/starter-kit": "^2.3.0",
    "@types/set-interval-async": "^1.0.3",
    "base64-arraybuffer": "^1.0.2",
    "fast-deep-equal": "^3.1.3",
    "fuse.js": "^7.0.0",
    "just-diff": "^6.0.2",
    "lodash": "^4.17.21",
    "mime-db": "^1.52.0",
    "nanoid": "^5.0.7",
    "rfdc": "^1.3.1",
    "rollup": "^4.15.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "set-interval-async": "^3.0.3",
    "svelte-grid": "^5.1.2",
    "svelte-i18n": "^4.0.0",
    "svelte-json-tree": "^2.2.0",
    "svelte-simple-modal": "^1.6.2",
    "sveltejs-tippy": "^3.0.0",
    "tinykeys": "^2.1.0",
    "uuid": "^9.0.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "tinykeys@2.1.0": "patches/tinykeys@2.1.0.patch"
    }
  }
}
