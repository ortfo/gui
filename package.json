{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "frontend-build": "vite build",
    "frontend-dev": "vite",
    "frontend-start": "vite preview",
    "frontend-check": "svelte-check --tsconfig ./tsconfig.json",
    "backend-build": "cd backend && go build",
    "backend-dev": "cd backend && nodemon --signal SIGINT --exec 'go run . || exit 1' -e go",
    "dev": "concurrently npm:frontend-dev npm:backend-dev",
    "format": "pnpm prettier frontend/ --write --plugin-search-dir=. && cd backend && gofmt -s -w *.go"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.40",
    "@tsconfig/svelte": "^2.0.1",
    "@types/colornames": "^1.1.2",
    "@types/lodash": "^4.14.181",
    "c8": "^7.11.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.4"
  },
  "dependencies": {
    "@geoffcox/svelte-splitter": "^1.0.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@tiptap/core": "^2.0.0-beta.174",
    "@tiptap/extension-bubble-menu": "^2.0.0-beta.55",
    "@tiptap/extension-image": "^2.0.0-beta.27",
    "@tiptap/extension-link": "^2.0.0-beta.36",
    "@tiptap/extension-placeholder": "^2.0.0-beta.48",
    "@tiptap/starter-kit": "^2.0.0-beta.183",
    "@types/set-interval-async": "^1.0.0",
    "base64-arraybuffer": "^1.0.2",
    "colornames": "^1.1.1",
    "fast-deep-equal": "^3.1.3",
    "just-diff": "^5.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.2",
    "rfdc": "^1.3.0",
    "rollup": "^2.70.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "set-interval-async": "^2.0.3",
    "svelte-grid": "^5.1.1",
    "svelte-json-tree": "^0.1.0",
    "svelte-simple-modal": "^1.3.1",
    "sveltejs-tippy": "^3.0.0",
    "tinykeys": "^1.4.0",
    "uuid": "^8.3.2"
  }
}
